<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:layout_marginTop="30dp"
    tools:context="kr.co.landvibe.handicraft.schedule.ScheduleFragment"
    android:orientation="vertical"
    >
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="스케쥴 관리"
        android:textSize="24sp"
        android:textAppearance="@style/TextAppearance.DOHYEON"
        />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="도현체"
        android:textSize="24sp"
        android:textAppearance="@style/TextAppearance.DOHYEON"
        />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="주아체"
        android:textSize="24sp"
        android:textAppearance="@style/TextAppearance.JUA"
        />
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="연성체"
        android:textSize="24sp"
        android:textAppearance="@style/TextAppearance.YEONSUNG"
        />
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="한나체"
        android:textSize="24sp"
        android:textAppearance="@style/TextAppearance.HANNA"
        />

</LinearLayout>